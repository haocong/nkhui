<% include _header %>
  <div class="cell">
    <div class="hd">
      <h1 class="page_title">完善资料</h1>
    </div>
    <div class="bd">
      <form action="/apply/teacher" method="post">
        <div class="weui_cells weui_cells_form">
          <div class="weui_cell">
            <div class="weui_cell_hd">
              <label class="weui_label">真实姓名</label>
            </div>
            <div class="weui_cell_bd weui_cell_primary">
              <input name="realname" class="weui_input" type="text" placeholder="请填写真实姓名" required autofocus>
            </div>
          </div>
          <div class="weui_cell">
            <div class="weui_cell_hd">
              <label class="weui_label">学号</label>
            </div>
            <div class="weui_cell_bd weui_cell_primary">
              <input name="studentid" class="weui_input" type="text" placeholder="请填写学号" required>
            </div>
          </div>
        </div>
        <div class="weui_cells_title">性别</div>
        <div class="weui_cells weui_cells_radio">
          <label class="weui_cell weui_check_label" for="male">
            <div class="weui_cell_bd weui_cell_primary">
              <p>男</p>
            </div>
            <div class="weui_cell_ft">
              <input type="radio" name="sex" id="male" class="weui_check" value="0" checked="checked">
              <span class="weui_icon_checked"></span>
            </div>
          </label>
          <label class="weui_cell weui_check_label" for="female">
            <div class="weui_cell_bd weui_cell_primary">
              <p>女</p>
            </div>
            <div class="weui_cell_ft">
              <input type="radio" name="sex" id="female" class="weui_check" value="1">
              <span class="weui_icon_checked"></span>
            </div>
          </label>
        </div>
        <div class="weui_cells_title">就读学校</div>
        <div class="weui_cells">
          <div class="weui_cell weui_cell_select">
            <div class="weui_cell_bd weui_cell_primary">
              <select name="schoolname" class="weui_select">
                <option value="南开大学" selected>南开大学</option>
              </select>
            </div>
          </div>
        </div>
        <div class="weui_cells_title">联系信息</div>
        <div class="weui_cells">
          <div class="weui_cell">
            <div class="weui_cell_hd">
              <label class="weui_label">+86</label>
            </div>
            <div class="weui_cell_bd weui_cell_primary">
              <input name="contactphone" class="weui_input" type="tel" placeholder="请输入号码" required>
            </div>
          </div>
          <div class="weui_cell">
            <div class="weui_cell_hd">
              <label class="weui_label">邮箱地址</label>
            </div>
            <div class="weui_cell_bd weui_cell_primary">
              <input name="email" class="weui_input" type="email" placeholder="请输入邮箱地址" required>
            </div>
          </div>
        </div>
        <div class="weui_cells_title">意向年级</div>
        <div class="weui_cells weui_cells_checkbox">
          <label class="weui_cell weui_check_label">
            <div class="weui_cell_hd">
              <input type="checkbox" name="intentlevel" class="weui_check" value="小学" checked="checked">
              <i class="weui_icon_checked"></i>
            </div>
            <div class="weui_cell_bd weui_cell_primary">
              <p>小学</p>
            </div>
          </label>
          <label class="weui_cell weui_check_label">
            <div class="weui_cell_hd">
              <input type="checkbox" name="intentlevel" class="weui_check" value="初中">
              <i class="weui_icon_checked"></i>
            </div>
            <div class="weui_cell_bd weui_cell_primary">
              <p>初中</p>
            </div>
          </label>
          <label class="weui_cell weui_check_label">
            <div class="weui_cell_hd">
              <input type="checkbox" name="intentlevel" class="weui_check" value="高中">
              <i class="weui_icon_checked"></i>
            </div>
            <div class="weui_cell_bd weui_cell_primary">
              <p>高中</p>
            </div>
          </label>
        </div>
        <div class="weui_cells_title">意向学科</div>
        <div class="weui_cells weui_cells_checkbox">
          <label class="weui_cell weui_check_label">
            <div class="weui_cell_hd">
              <input type="checkbox" name="intentsubject" class="weui_check" value="数学" checked="checked">
              <i class="weui_icon_checked"></i>
            </div>
            <div class="weui_cell_bd weui_cell_primary">
              <p>数学</p>
            </div>
          </label>
          <label class="weui_cell weui_check_label">
            <div class="weui_cell_hd">
              <input type="checkbox" name="intentsubject" class="weui_check" value="语文">
              <i class="weui_icon_checked"></i>
            </div>
            <div class="weui_cell_bd weui_cell_primary">
              <p>语文</p>
            </div>
          </label>
          <label class="weui_cell weui_check_label">
            <div class="weui_cell_hd">
              <input type="checkbox" name="intentsubject" class="weui_check" value="英语">
              <i class="weui_icon_checked"></i>
            </div>
            <div class="weui_cell_bd weui_cell_primary">
              <p>英语</p>
            </div>
          </label>
        </div>
        <div class="weui_cells_title">课外项目</div>
        <div class="weui_cells weui_cells_checkbox">
          <label class="weui_cell weui_check_label">
            <div class="weui_cell_hd">
              <input type="checkbox" name="extendsubject" class="weui_check" value="健身" checked="checked">
              <i class="weui_icon_checked"></i>
            </div>
            <div class="weui_cell_bd weui_cell_primary">
              <p>健身</p>
            </div>
          </label>
          <label class="weui_cell weui_check_label">
            <div class="weui_cell_hd">
              <input type="checkbox" name="extendsubject" class="weui_check" value="跆拳道">
              <i class="weui_icon_checked"></i>
            </div>
            <div class="weui_cell_bd weui_cell_primary">
              <p>跆拳道</p>
            </div>
          </label>
          <label class="weui_cell weui_check_label">
            <div class="weui_cell_hd">
              <input type="checkbox" name="extendsubject" class="weui_check" value="演讲">
              <i class="weui_icon_checked"></i>
            </div>
            <div class="weui_cell_bd weui_cell_primary">
              <p>演讲</p>
            </div>
          </label>
          <label class="weui_cell weui_check_label">
            <div class="weui_cell_hd">
              <input type="checkbox" name="extendsubject" class="weui_check" value="单片机制作">
              <i class="weui_icon_checked"></i>
            </div>
            <div class="weui_cell_bd weui_cell_primary">
              <p>单片机制作</p>
            </div>
          </label>
          <label class="weui_cell weui_check_label">
            <div class="weui_cell_hd">
              <input type="checkbox" name="extendsubject" class="weui_check" value="3D打印机电模">
              <i class="weui_icon_checked"></i>
            </div>
            <div class="weui_cell_bd weui_cell_primary">
              <p>3D打印机电模</p>
            </div>
          </label>
          <label class="weui_cell weui_check_label">
            <div class="weui_cell_hd">
              <input type="checkbox" name="extendsubject" class="weui_check" value="书法绘画">
              <i class="weui_icon_checked"></i>
            </div>
            <div class="weui_cell_bd weui_cell_primary">
              <p>书法绘画</p>
            </div>
          </label>
        </div>
        <br>
        <div class="bd spacing">
          <input type="submit" value="提交" class="weui_btn weui_btn_primary">
        </div>
      </form>
    </div>
  </div>
<% include _footer %>
